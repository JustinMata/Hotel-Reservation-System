import java.util.ArrayList;

public class Guest extends User{
	private ArrayList<Reservations> reserve;
	
	
	public Guest(int id, String password, String username) {
		super(id, password, username);
		// TODO Auto-generated constructor stub
	}


	public ArrayList<Reservations> getReservations() {
		return reserve;
	}


	public void addReservations(Reservation r) {
		this.reserve = reserve;
	}
	
	

}
